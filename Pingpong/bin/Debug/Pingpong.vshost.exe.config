      <wellknown type = "gamelogic.Game, gamelogic"
                          url="tcp://localhost:9000/Game/Gameee" />
    </client>
    <channels>
      <channel ref="tcp" displayName="TCP Channel (GameClient)" >
        <serverProviders>
          <formatter ref="binary" typeFilterLevel="Full"/>
        </serverProviders>
        <clientProviders>
          <formatter ref="binary"/>
        </clientProviders>
      </channel>
    </channels>
  </application>
